{
  "name": "duti",
  "description": "Danger UTIlities",
  "bin": {
    "duti": "bin/duti.js"
  },
  "version": "0.2.1",
  "main": "src/index.js",
  "license": "MIT",
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "test": "jest",
    "test:watch": "npm t -- --watch",
    "test:ci": "npm t -- --coverage --json --outputFile=test-results.json",
    "lint": "eslint ./**/*.js",
    "lint:ci": "npm run lint -- -o lint-results.json -f json",
    "danger": "./bin/duti.js",
    "fmt": "prettier --write {*.js,{src,bin}/**/*.js} --no-semi --single-quote --trailing-comma all"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "env": {
      "node": true
    },
    "parserOptions": {
      "ecmaVersion": 8,
      "sourceType": "module"
    }
  },
  "dependencies": {
    "bluebird": "^3.5.0",
    "danger": "^1.1.0",
    "futil-js": "^1.27.0",
    "lodash": "^4.17.4",
    "pkg-up": "^2.0.0",
    "strip-ansi": "^4.0.0"
  },
  "devDependencies": {
    "eslint": "^4.3.0",
    "jest": "^20.0.4",
    "prettier": "^1.5.3"
  }
}
